#include <main.h>

extern volatile uint32_t dma2_status;
float32_t sensor_data;
extern volatile uint32_t adc_data[3];




int main(void){



	adc_init();
	adc_start();

	while(1){
		uint32_t adc_cdr_value = ADC->CDR;

		// Extract individual ADC values
		uint16_t adc1_value = (adc_cdr_value & 0xFFFF);          // ADC1 result
		uint16_t adc2_value = (adc_cdr_value >> 16) & 0xFFFF;    // ADC2 result
		uint16_t adc3_value = (adc_cdr_value >> 16) & 0xFFFF;    // ADC3 result







	}
}

