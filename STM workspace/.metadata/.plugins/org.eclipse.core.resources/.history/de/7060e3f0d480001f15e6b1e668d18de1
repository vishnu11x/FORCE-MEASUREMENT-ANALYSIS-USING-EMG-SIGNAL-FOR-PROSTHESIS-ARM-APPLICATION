#include <switch.h>

void switch_init(void){

	RCC-> AHB1ENR |= (RCC_AHB1ENR_GPIOAEN);  //Enable Clock for GPIO

	//Select Input mode for PA5
	GPIOA -> MODER &= ~( (GPIO_MODER_MODE5_0) | (GPIO_MODER_MODE5_1) );
//	GPIOA -> PUPDR &= (GPIO_PUPDR_PUPD5_0);
//	GPIOA -> PUPDR |= (GPIO_PUPDR_PUPD5_1);

}
